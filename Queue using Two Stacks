#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <stack>
#include <algorithm>

using namespace std;

int main() {
    int q;
    cin >> q;
    stack<int> stack_in, stack_out;

    while (q--) {
        int type;
        cin >> type;

        if (type == 1) {
            int x;
            cin >> x;
            stack_in.push(x);  // enqueue
        } else if (type == 2) {
            // dequeue
            if (stack_out.empty()) {
                while (!stack_in.empty()) {
                    stack_out.push(stack_in.top());
                    stack_in.pop();
                }
            }
            stack_out.pop();
        } else if (type == 3) {
            // print front
            if (stack_out.empty()) {
                while (!stack_in.empty()) {
                    stack_out.push(stack_in.top());
                    stack_in.pop();
                }
            }
            cout << stack_out.top() << endl;
        }
    }

    return 0;
}
